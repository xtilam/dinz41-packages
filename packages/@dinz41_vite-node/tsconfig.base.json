{
  "compilerOptions": {
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "module": "ESNext",
    "target": "ESNext",
    "strict": false,
    "allowJs": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowImportingTsExtensions": false,
    "emitDeclarationOnly": true,
    "declaration": true,
    "allowUmdGlobalAccess": true,
    "esModuleInterop": true,
  }
}
